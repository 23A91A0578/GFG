/*
class Node {
    int data;
    Node next;
    Node(int value) {
        this.value = value;
    }
}
*/

class Solution {
    Node reverseList(Node head) {
        // code here
        Node prev=null;
        Node curr=head;
        Node cur2;
        while(curr!=null){
            cur2=curr.next;
            curr.next=prev;
            prev=curr;
            curr=cur2;
        }
        return prev;        
    }
}
