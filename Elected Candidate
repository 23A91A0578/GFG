class Solution {
    public static String electionWinner(String[] arr) {
        HashMap<String, Integer> voteCount = new HashMap<>();
        String winner = "";
        int maxVotes = 0;

        for (String name : arr) {
            voteCount.put(name, voteCount.getOrDefault(name, 0) + 1);
            int votes = voteCount.get(name);

            if (votes >= maxVotes ) {
                winner = name;
                maxVotes = votes;
            }
        }

        return winner;
    }
}
