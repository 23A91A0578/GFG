/*
class Node {
    int data;
    Node next;
    Node prev;

    Node(int data) {
        this.data = data;
        this.next = null;
        this.prev = null;
    }
}
*/
class Solution {
    public Node reverse(Node head) {
        // code here
        Node prev = null, current = head, next = null;
        while (current != null) {
        next = current.next; // Store next node
        current.next = prev; // Reverse link
        prev = current; // Move prev forward
        current = next; // Move current forward
        }
        return prev;
    }
}
